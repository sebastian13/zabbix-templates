<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2020-01-04T10:26:22Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Rescript Restic Backup</template>
            <name>Template Rescript Restic Backup</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <discovery_rules>
                <discovery_rule>
                    <name>LV Discovery</name>
                    <type>TRAP</type>
                    <key>rescript.lv.discovery</key>
                    <delay>0</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Restic Backup of LV {#LV}: Bytes Added ({#REPO})</name>
                            <type>TRAP</type>
                            <key>restic.backup.added.[{#LV}.{#REPO}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Restic Backup of {#LV} to {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Restic Backup of LV {#LV}: Bytes Processed ({#REPO})</name>
                            <type>TRAP</type>
                            <key>restic.backup.processedbytes.[{#LV}.{#REPO}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Restic Backup of {#LV} to {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Restic Backup of LV {#LV}: Processed Time ({#REPO})</name>
                            <type>TRAP</type>
                            <key>restic.backup.processedtime.[{#LV}.{#REPO}]</key>
                            <delay>0</delay>
                            <units>s</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Restic Backup of {#LV} to {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Restic Backup of LV {#LV}: Snapshot-ID ({#REPO})</name>
                            <type>TRAP</type>
                            <key>restic.backup.snapshotid.[{#LV}.{#REPO}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Restic Backup of {#LV} to {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{nodata(2d)}=1</expression>
                                    <name>Restic LVM Backup of {#LV} to {#REPO} is outstanding on {HOST.NAME}</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Repo Discovery</name>
                    <type>TRAP</type>
                    <key>rescript.repo.discovery[discoverrepos]</key>
                    <delay>0</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Rescript Backup: Bytes Added ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.backup.added[{#REPO}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Backup: Processed Bytes ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.backup.processedbytes[{#REPO}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Backup: Processed Time to ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.backup.processedtime[{#REPO}]</key>
                            <delay>0</delay>
                            <units>s</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Backup: Snapshot-ID ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.backup.snapshotid[{#REPO}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{nodata(2d)}=1</expression>
                                    <name>Rescript Restic Backup on {HOST.NAME} to {#REPO} is outstanding</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Check: Message ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.check.message[{#REPO}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{regexp(failed)}=1</expression>
                                    <name>Rescript Restic Check on {HOST.NAME} to {#REPO} failed</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Checksum of {#REPO}.conf</name>
                            <type>TRAP</type>
                            <key>rescript.config.cksum[{#REPO}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{diff(0)}&gt;0</expression>
                                    <name>Rescript Restic config has been changed on {HOST.NAME} to {#REPO}</name>
                                    <priority>WARNING</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Prune: Number of Packs Deleted ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.prune.delpacks[{#REPO}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Prune: Freed Space ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.prune.freedspace[{#REPO}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Prune: Number of Packs ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.prune.packs[{#REPO}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Prune: Policy ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.prune.policy[{#REPO}]</key>
                            <delay>0</delay>
                            <trends>0</trends>
                            <value_type>CHAR</value_type>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Prune: Invalid Files Removed ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.prune.remfiles[{#REPO}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Prune: Number of Packs Rewritten ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.prune.rewpacks[{#REPO}]</key>
                            <delay>0</delay>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>Rescript Prune: Total Size ({#REPO})</name>
                            <type>TRAP</type>
                            <key>rescript.prune.size[{#REPO}]</key>
                            <delay>0</delay>
                            <units>B</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Rescript Restic Backup: {#REPO}</name>
                                </application_prototype>
                            </application_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
